  			<xs:complexType>
    		<xs:simpleContent>
      		<xs:extension base="xs:int">
        	<xs:attribute name="korzet" type="xs:int" default="48" />
   			</xs:extension>
   			</xs:simpleContent>
  			</xs:complexType>
		</xs:element>
		<xs:element name="email" type="xs:string"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:int"/>
		</xs:complexType>
		</xs:element>
	</xs:sequence>
	</xs:complexType>
 
 
 
 
 
 
 </xs:element>


</xs:schema>